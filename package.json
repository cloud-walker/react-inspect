{
  "name": "@cloudwalker/react-inspect",
  "version": "0.4.0",
  "description": "Because json stringify is not enough",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "storybook": "start-storybook -p 9000 -c .storybook",
    "test": "jest",
    "prepublish": "rollup -c"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "jsnext:main": "dist/react-inspect.es.js",
  "module": "dist/react-inspect.es.js",
  "main": "dist/react-inspect.cjs.js",
  "author": "Luca Barone <baro.luc@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloud-walker/react-inspect"
  },
  "bugs": {
    "url": "https://github.com/cloud-walker/react-inspect/issues"
  },
  "keywords": [
    "react",
    "inspect",
    "log",
    "json",
    "stringify"
  ],
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=15"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.8",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "jest": "^21.0.2",
    "np": "^2.16.0",
    "prettier": "^1.5.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "rollup": "^0.49.3",
    "rollup-analyzer-plugin": "^1.0.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "sanitize.css": "^5.0.0"
  },
  "dependencies": {
    "just-is-circular": "^1.0.0",
    "ramda": "^0.24.1",
    "styled-components": "^2.1.2"
  }
}
